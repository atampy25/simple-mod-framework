{
	"name": "ModManager",
	"version": "1.0.0",
	"private": true,
	"description": "Mod manager for the Simple Mod Framework.",
	"main": "src/electron.cjs",
	"type": "module",
	"scripts": {
		"dev": "cross-env NODE_ENV=dev npm run dev:all",
		"dev:all": "concurrently -n=svelte,electron -c='#ff3e00',blue \"npm run dev:svelte\" \"npm run dev:electron\"",
		"dev:svelte": "svelte-kit dev",
		"dev:electron": "electron src/electron.cjs",
		"build": "cross-env NODE_ENV=production npm run build:svelte && npm run build:electron",
		"build:svelte": "svelte-kit build",
		"build:electron": "electron-builder -w --dir --config build.config.json",
		"start": "svelte-kit start"
	},
	"engines": {
		"npm": ">=7",
		"pnpm": "use npm - https://github.com/FractalHQ/sveltekit-electron/issues/12#issuecomment-1068399385",
		"yarn": "use npm - https://github.com/FractalHQ/sveltekit-electron/issues/12#issuecomment-1068399385"
	},
	"browserslist": [
		"Chrome 89"
	],
	"dependencies": {
		"@electron/remote": "^2.0.8",
		"@fortawesome/free-regular-svg-icons": "^6.1.1",
		"@fortawesome/free-solid-svg-icons": "^6.1.1",
		"adm-zip": "^0.5.9",
		"autoprefixer": "^10.4.7",
		"axios": "^0.27.2",
		"carbon-components-svelte": "^0.64.1",
		"carbon-icons-svelte": "^11.0.1",
		"chalk": "^4.1.2",
		"electron-context-menu": "^2.5.2",
		"electron-reloader": "^1.2.3",
		"electron-serve": "^1.1.0",
		"electron-window-state": "^5.0.3",
		"fs-extra": "^10.1.0",
		"json5": "^2.2.1",
		"klaw-sync": "^6.0.0",
		"lodash.chunk": "^4.2.0",
		"lodash.get": "^4.4.2",
		"lodash.isequal": "^4.5.0",
		"lodash.memoize": "^4.1.2",
		"lodash.mergewith": "^4.6.2",
		"marked": "^4.0.16",
		"md5": "^2.3.0",
		"postcss": "^8.4.14",
		"rehype-stringify": "^9.0.3",
		"remark": "^14.0.2",
		"remark-gfm": "^3.0.1",
		"remark-parse": "^10.0.1",
		"remark-rehype": "^10.1.0",
		"remark-torchlight": "^0.0.2",
		"semver": "^7.3.7",
		"svelte-fa": "^2.4.0",
		"svelte-sortable-list": "^1.1.0",
		"svelte-tippy": "^1.3.2",
		"tailwindcss": "^3.0.24",
		"tippy.js": "^6.3.7",
		"unified": "^10.1.2",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^1.0.0-next.29",
		"@sveltejs/kit": "next",
		"@types/adm-zip": "^0.5.0",
		"@types/electron-window-state": "^2.0.34",
		"@types/lodash.isequal": "^4.5.6",
		"@types/lodash.memoize": "^4.1.7",
		"@types/lodash.mergewith": "^4.6.7",
		"@types/marked": "^4.0.3",
		"@types/uuid": "^8.3.4",
		"@typescript-eslint/eslint-plugin": "^5.26.0",
		"@typescript-eslint/parser": "^4.33.0",
		"concurrently": "^7.2.1",
		"cross-env": "^7.0.3",
		"dotenv": "^8.6.0",
		"electron": "^18.1.0",
		"electron-builder": "^23.0.3",
		"electron-connect": "^0.6.3",
		"electron-packager": "^15.4.0",
		"electron-updater": "^4.6.5",
		"eslint": "^8.15.0",
		"eslint-plugin-svelte": "^1.1.2",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.6.2",
		"prettier-eslint": "^15.0.0",
		"prettier-plugin-svelte": "^2.7.0",
		"sass": "^1.52.1",
		"svelte": "^3.47.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"typescript": "^4.7.2",
		"vite": "^2.9.9"
	}
}

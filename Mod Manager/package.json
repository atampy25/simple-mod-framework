		"electron-updater": "^4.6.5",
		"eslint": "^8.21.0",
		"eslint-plugin-svelte": "^2.33.2",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.0.3",
		"prettier-eslint": "^15.0.1",
		"prettier-plugin-svelte": "^3.0.3",
		"sass": "^1.54.4",
		"svelte": "^4.2.1",
		"svelte-check": "^3.5.2",
		"svelte-preprocess": "^5.0.4",
		"typescript": "^4.7.4",
